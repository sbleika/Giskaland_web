# EditorConfig: http://EditorConfig.org
# This file is used to maintain consistent coding style between different editors and IDEs
root = true

[*]
indent_style = space
indent_size = 4
indent_style = tab
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
